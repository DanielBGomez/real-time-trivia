{
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "test"
        ],
        "accessToken": "MDlmMmE1ZGYtNmU3OS00MzU4LTgwYTItMjQwNGQyNjE0NGY0fHJlYWQtd3JpdGU="
      }
    }
  },
  "namedInputs": {
    "all": ["{projectRoot}/**/*"],
    "public": ["!{projectRoot}/**/*/*.test.*"]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": ["public", "^public"],
      "outputs": [
        "{projectRoot}/lib"
      ]
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": ["all", "^public"]
    }
  },
  "defaultBase": "master"
}
